
#logger {
	display: inline-block;
	float: left;
	width: 600px;
	height: 315px;
}


ul#sensorlist {
	display:inline-block;
	margin-left: 20px;
	padding: 0px;
	list-style: none;
	font: 700 18px "Arial";
}

ul#sensorlist li {
	position: relative;
	min-height: 25px;
	background-color: #BFE3FF;
	margin: 1px;	
	padding: 5px 15px;
}

ul#sensorlist li input[type="checkbox"] {
	border: 1px solid #4CB2FF;
}

ul#sensorlist li input[type="checkbox"]:checked {
	background-position: 0 0;
}

ul#sensorlist li label {
	margin-left: 15px;
	padding-top: 3px;
}

input[type="text"] {
	position: absolute;
	width: 80px;
	border: 1px solid #4CB2FF;
	right: 15px;
	font-size: 14px;
	height: 24px;
	text-align:right;
	padding-right: 2px;
}

select {
	position: absolute;
	right: 15px;
	width: 84px;
	height: 26px;
	border: 1px solid #4CB2FF;
	font: 500 14px "Arial" !important;
	padding: 3px 0px 3px 2px;
	background-color: #fff;
}

